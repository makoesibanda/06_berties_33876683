<!doctype html>
<html>
  <head>
    <title>Registered Users â€“ <%= shopData.shopName %></title>
    <link rel="stylesheet" type="text/css" href="/main.css">
  </head>
  <body>
    <h1>Registered Users for <%= shopData.shopName %></h1>

    <table>
      <tr>
        <th>Username</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
      </tr>

      <% usersData.forEach(function(user) { %>
        <tr>
          <td><%= user.username %></td>
          <td><%= user.first %></td>
          <td><%= user.last %></td>
          <td><%= user.email %></td>
        </tr>
      <% }) %>
    </table>

    <p><a href="/users/register">Register another user</a></p>
    <p><a href="/">Back to Home</a></p>
  </body>
</html>
